{
  "name": "@lunaticenslaved/schema",
  "version": "0.0.33",
  "main": "dist/umd/main.js",
  "types": "dist/umd/main.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": {
        "require": "./dist/umd/main.d.ts",
        "default": "./dist/umd/main.d.ts"
      },
      "browser": {
        "require": "./dist/browser/main.cjs",
        "default": "./dist/browser/main.cjs"
      },
      "default": {
        "require": "./dist/node/main.cjs",
        "default": "./dist/node/main.cjs"
      }
    },
    "./.prettierrc.js": "./.prettierrc.js",
    "./.eslintrc.js": "./.eslintrc.js",
    "./tsconfig.base.json": "./tsconfig.base.json",
    "./actions": {
      "types": {
        "require": "./dist/umd/actions/index.d.ts",
        "default": "./dist/umd/actions/index.d.ts"
      },
      "browser": {
        "require": "./dist/browser/main.cjs",
        "default": "./dist/browser/main.cjs"
      },
      "default": {
        "require": "./dist/node/main.cjs",
        "default": "./dist/node/main.cjs"
      }
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --noEmit && npx rollup --config rollup.config.mjs",
    "lint:fix": "eslint --fix --max-warnings=0 . && prettier . --write"
  },
  "author": "lunaticenslaved",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.201",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.0.3",
    "rollup": "^4.5.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-node-externals": "^6.1.2",
    "tsc-alias": "^1.8.8",
    "typescript": "5.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.21"
  }
}
